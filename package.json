{
  "author": "Rowan Wookey <admin@rwky.net> (https://www.rwky.net)",
  "name": "mysql-abstraction",
  "description": "Abstraction layer on top of Felix G's mysql driver",
  "version": "5.1.3",
  "keywords": "mysql",
