    "mysql": "2.18.x"
  },
  "main": "lib/index",
  "devDependencies": {
    "async": "^3.2.0",
    "chai": "4.2.x",
    "coveralls": "^3.0.5",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.0",
    "nyc": "^15.1.0"
  },
  "repository": {
